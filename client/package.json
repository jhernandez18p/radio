{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animated": "^0.2.2",
    "auth0-js": "^9.5.1",
    "bulma": "^0.7.1",
    "bulma-extensions": "^1.1.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "next": "^6.0.3",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-slick": "^0.23.1",
    "react-transition-group": "^2.3.1",
